{
  "name": "pandorica-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./src/server.js --exec babel-node",
    "build": "babel ./src -d ./dist",
    "serve": "NODE_ENV=production forever start ./dist/server.js",
    "migrate": "babel-node ./node_modules/mongodb-migrate -runmm -c ./src/db -cfg ./config.json -dbn $NODE_ENV",
    "restart": "NODE_ENV=production forever restart ./dist/server.js"
  },
  "dependencies": {
    "api-error-handler": "^1.0.0",
    "babel-cli": "^6.4.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-strict-mode": "^6.6.5",
    "babel-preset-es2016": "^6.0.11",
    "body-parser": "~1.13.2",
    "co-express": "^1.2.1",
    "compression": "^1.6.2",
    "cookie-parser": "~1.3.5",
    "cors": "^2.7.1",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "gm": "^1.22.0",
    "http-errors": "^1.5.0",
    "jade": "~1.11.0",
    "kerberos": "0.0.18",
    "mongo-migrate": "^1.3.0",
    "mongoose": "^4.3.7",
    "mongoose-findorcreate": "^0.1.2",
    "morgan": "~1.6.1",
    "passport": "^0.3.2",
    "passport-google-oauth": "^0.2.0",
    "passport-google-token": "^0.1.2",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "promised-io": "^0.3.5",
    "qr-image": "^3.1.0",
    "randomstring": "^1.1.4",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.4.8",
    "validator": "^5.1.0"
  },
  "devDependencies": {
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "nodemon": "^1.9.2"
  }
}
